#include <stdio.h>

int main() {
    int rows, cols;

    printf("Enter number of rows and columns: ");
    scanf("%d %d", &rows, &cols);

    int matrix[100][100]; 

    printf("Enter the elements of the matrix:\n");
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    long long product = 1;
    for (int i = 0; i < rows; i++) {
        product *= matrix[i][0];
    }

    printf("Multiplication of all elements in the first column = %lld\n", product);

    return 0;
}
